    <div class="content-area submit-property" style="background-color: #FCFCFC;">&nbsp;
        <div class="container">
            <div class="clearfix" > 
                <div class="wizard-container"> 

                    <div class="wizard-card ct-wizard-orange" id="wizardProperty">
                        <form method="post" enctype="multipart/form-data">                        
                            <div class="wizard-header">
                                <h3>
                                    <b>Add</b> YOUR PROPERTY <br>
                                    <small>Publish your property with sample click.</small>
                                </h3>
                            </div>


                            <ul>
                                <li><a href="#step1" data-toggle="tab">Step 1 </a></li>
                                <li><a href="#step2" data-toggle="tab">Step 2 </a></li>
                                <li><a href="#step3" data-toggle="tab">Step 3 </a></li>
                                <li><a href="#step4" data-toggle="tab">Finished </a></li>
                            </ul>

                             <div class="tab-content">

                                    <div class="tab-pane" id="step1">
                                        <div class="row p-b-15">
                                        <h4 class="info-text"> Let's start with the basic information of your property</h4>

                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label>Property Type<small>(required)</small></label><br>
                                                <select name="propertyType"  id="lunchBegins" class="selectpicker" data-live-search="true" data-live-search-style="begins" title="Select your property type">
                                                    <option value="Appartement">Appartement</option>
                                                    <option value="Condo">Condo</option>
                                                    <option value="House">House</option>
                                                    <option value="Store">Store</option>
                                                    <option value="Villa">Villa</option>


                                                </select>
                                            </div>

                                            <div class="form-group">
                                                <label>Property price <small>(required)</small></label>
                                                <input type="number" min="0" max="999999" step="0.01" name="squreFeet" value="{{v.price}}" class="form-control" placeholder="3330000">
                                            </div> 

                                        </div>
                                    </div>
                                </div>
                                <!--  End step 1 -->

                                <div class="tab-pane" id="step2">
                                    <h4 class="info-text"> How much your Property is Beautiful ? </h4>
                                    <div class="row">
                                        <div class="col-sm-12"> 
                                            <div class="col-sm-4"> 
                                                <div class="form-group">
                                                    <label>Property Latitude :</label>
                                                    <input type="number" min="-90 " max="+90 " step="0.01" name="latitude" value="{{v.latitude}}" class="form-control" placeholder="3330000">
                                                </div> 
                                            </div> 
                                        </div>

                                        <div class="col-sm-12">
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label>Property Longitude :</label>
                                                    <input type="number" min="-180 " max="+180 " step="0.01" name="longitude" value="{{v.longitude}}" class="form-control" placeholder="3330000">
                                                </div>
                                            </div>


                                        </div>
                                        <div class="col-sm-12 padding-top-15">                                                   
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label for="property-geo">Min bed :</label>
                                                    <input type="number" min="0" max="10" step="1" name="beds" value="{{v.beds}}" class="form-control" ><br />

                                                </div>
                                            </div>
                                            <div class="col-sm-4">

                                                <div class="form-group">
                                                    <label for="price-range">Min baths :</label>
                                                    <input type="number" min="0" max="10" step="1" name="baths" value="{{v.baths}}" class="form-control" ><br />

                                                </div>
                                            </div>
                                            <div class="col-sm-4">

                                                <div class="form-group">
                                                    <label for="property-geo">Property geo (m2) :</label>
                                                    <input type="number" min="0" max="999999" step="0.01" name="price" value="{{v.squreFeet}}" class="form-control"><br />

                                                </div>
                                            </div>   
                                        </div>

                                    </div>
                                </div>
                                <!-- End step 2 -->

                                <div class="tab-pane" id="step3">                                        
                                    <h4 class="info-text">Give us  image ? </h4>
                                    <div class="row">  
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label for="property-images">Choose Image :</label>

                                                <input class="form-control" type="file" name="propertyImage"  id="property-images">
                                                <p class="help-block">Select image for your property .</p>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <!--  End step 3 -->


                                <div class="tab-pane" id="step4">                                        
                                    <h4 class="info-text"> Finished and submit </h4>
                                    <div class="row">  
                                        <div class="col-sm-12">
                                            <div class="">
                                                <p>
                                                    <label><strong>Terms and Conditions</strong></label>
                                                    By accessing or using  Real ESTATE services, such as 
                                                    posting your property advertisement with your personal 
                                                    information on our website you agree to the
                                                    collection, use and disclosure of your personal information 
                                                    in the legal proper manner
                                                </p>

                                                <div class="checkbox">
                                                    <label>
                                                        <input type="checkbox" /> <strong>Accept termes and conditions.</strong>
                                                    </label>
                                                </div> 

                                            </div> 
                                        </div>
                                    </div>
                                </div>
                                <!--  End step 4 -->

                            </div>

                            <div class="wizard-footer">
                                <div class="pull-right">
                                    <input type='button' class='btn btn-next btn-primary' name='next' value='Next' />
                                    <input type='button' class='btn btn-finish btn-primary ' name='finish' value='Finish' />
                                </div>

                                <div class="pull-left">
                                    <input type='button' class='btn btn-previous btn-default' name='previous' value='Previous' />
                                </div>
                                <div class="clearfix"></div>                                            
                            </div>	
                        </form>
                    </div>
                    <!-- End submit form -->
                </div> 
            </div>
        </div>
