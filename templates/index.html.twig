                                        <option>Rent </option>
                                        <option>Buy</option>
                                        

                                    </select>
                                </div>
                                <a class="btn search-btn" type="submit" href="/property/search"><i class="fa fa-search"></i></a>

                                <div style="display: none;" class="search-toggle">

                                    <div class="search-row">   

                                        <div class="form-group mar-r-20">
                                            <label for="price-range">Latitude Range Start:</label>
